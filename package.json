{
  "name": "@magicappdev/mobile",
  "version": "0.0.5",
  "private": true,
  "description": "An mobile app built with Ionic to build apps with MagicAppDev",
  "type": "module",
  "scripts": {
    "android": "ionic capacitor run android -l --external",
    "prebuild": "npm run format && npm run lint:fix && npm run typecheck",
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ionic:build": "npm run build",
    "ionic:sync": "npx cap sync",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@capacitor/android": "8.0.2",
    "@capacitor/app": "8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "8.0.2",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/status-bar": "8.0.0",
    "@eslint/js": "^9.39.2",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "history": "^4.10.0",
    "ionicons": "^7.4.0",
    "prettier": "^3.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.2",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.3",
    "cypress": "^13.5.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.24.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
